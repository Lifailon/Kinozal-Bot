<script>
toLinks('lnks_brs', createFilmLink);
toLinks('lnks_dbrs', createDiscographyLink);

// для элементов, имеющих указанный класс, преобразует текст в ссылки через указанный конвертер
function toLinks(className, converter) {
    var elements = document.getElementsByClassName(className);
    for (var i = 0; i < elements.length; i++) {
        var lines = elements[i].innerHTML.split('<br>');
        var result = [];
        for (var j = 0; j < lines.length; j++) {
            // преобразуем текст в ссылку
            result[j] = converter(lines[j]);
        }
        elements[i].innerHTML = result.join('<br>');
    }
}

// создает ссылку на фильм
function createFilmLink(line) {
    var year = getYear(line);
    if (!year) {
        return line;
    }

    // готовим заголовок ссылки
    var title = line
        .replace(/"/g, '')
        .replace(/[–—–]/g, '-')
        .replace(/…/g, '...');

    // убираем из ссылки " ... <исполняемая роль>", если она есть
    var dotsPos = title.lastIndexOf(' ...');
    var restText = '';
    if (dotsPos > 9) {
        restText = title.substring(dotsPos);
        title = title.substring(0, dotsPos);
    }

    // готовим строку поиска
    var search = title.replace(/\d{4}(\s*-\s*(\d{4}|\.\.\.))?\s+/g, '');

    // обрезаем ее до ( и /
    search = search.split(" (")[0];
    search = search.split(" / ")[0];

    // формируем ссылку и текст
    return '<a href="/browse.php?s=' + search.trim() + '&d=' + year + '" class="sba" target=_blank>' + title + '</a>' + restText;
}

function createDiscographyLink(line) {
    var year = getYear(line);
    if (!year) {
        return line;
    }
    return '<a href="/browse.php?s=Лина Хиди&g=1&c=1004" class="sba" target=_blank>' + line + '</a>';
}

// начинается ли строка с года?
function getYear(line) {
    var matches = line.match(/^\s*(\d{4}) /);
    return matches ? matches[1] : null;
}
</script>
