<script>
    function hashCode(domain){
        if (domain.length === 0) return 0;
        var hash = 0
        var chr;

        for (var i = 0; i < domain.length; i++) {
            chr = domain.charCodeAt(i);
            hash = ((hash << 5) - hash) + chr;
            hash |= 0;
        }
        return hash;
    }
    setTimeout(function() {
     //   var code = hashCode(location.hostname);
     //   if(code != 1393606474 && code != -774359431 && code != 1393606240) return;
    
